<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph_collection_activity"
    app:startDestination="@id/MapsFragment">

    <fragment
        android:id="@+id/MapsFragment"
        android:name="com.example.greenaura.MapsFragment"
        android:label="Maps Fragment"
        tools:layout="@layout/fragment_maps">
        <action
            android:id="@+id/action_MapsFragment_to_ReminderFragment"
            app:destination="@id/DestReminderFragment" />
    </fragment>

    <fragment
        android:id="@+id/DestReminderFragment"
        android:name="com.example.greenaura.ReminderFragment"
        android:label="fragment_g_w_reminder"
        tools:layout="@layout/fragment_reminder" />

    <activity
        android:id="@+id/collectionActivity"
        android:name="com.example.greenaura.CollectionActivity"
        android:label="activity_collection"
        tools:layout="@layout/activity_collection" />

</navigation>